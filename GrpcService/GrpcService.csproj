<Project Sdk="Microsoft.NET.Sdk.Web">

  <PropertyGroup>
    <TargetFramework>net5.0</TargetFramework>
  </PropertyGroup>

  <ItemGroup>
    <None Remove="Protos\Account\Get.proto" />
    <None Remove="Protos\Account\Model.proto" />
  </ItemGroup>

  <ItemGroup>
    <Protobuf Include="Protos\Account\Get.proto" GrpcServices="Server" />
    <Protobuf Include="Protos\Account\Model.proto" GrpcServices="Server" />
