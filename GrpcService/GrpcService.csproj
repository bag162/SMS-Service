  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="Grpc.AspNetCore" Version="2.34.0" />
  </ItemGroup>

  <ItemGroup>
    <Folder Include="Protos\User\" />
    <Folder Include="Protos\Order\" />
    <Folder Include="Protos\History\" />
    <Folder Include="Protos\Proxy\" />
    <Folder Include="Protos\Prices\" />
  </ItemGroup>

  <ItemGroup>
    <ProjectReference Include="..\Implemantation\Implemantation.csproj" />
  </ItemGroup>

</Project>
